<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="rectangle">

            <padding
                android:right="10dp"
                android:left="10dp"
                android:bottom="15dp"
                android:top="15dp"/>
            <stroke
                android:width="1dp"
                android:color="@color/et_txt" />
            <solid
                android:color="@color/colorWhite" />
            <corners
                android:radius="4dp"/>
        </shape>
    </item>
</layer-list>