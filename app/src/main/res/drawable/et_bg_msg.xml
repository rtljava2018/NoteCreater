<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="rectangle">
            <padding
                android:right="5dp"
                android:left="5dp"
                android:bottom="5dp"
                android:top="5dp"/>
            <stroke
                android:width="1dp"
                android:color="@color/et_txt" />
            <solid
                android:color="@color/colorWhite" />
            <corners
                android:radius="1dp"/>
        </shape>
    </item>
</layer-list>